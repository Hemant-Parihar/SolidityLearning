pragma solidity ^0.8.7;

 // Factorial of a number.
contract SolidityStorage {

    function fact(uint256 num) public pure returns (uint256) {
        uint256 prod = 1;
        while(num > 1) {
            prod *= num;
            num -= 1;
        }
        return prod;
    }
}
